issn = {0003-0147},
journal = {The American Naturalist},
keywords = {adaptive radiation,contingency,ecological opportunity},
number = {6},
pages = {623--639},
pmid = {20412015},
title = {{Adaptive Radiation, Ecological Opportunity, and Evolutionary Determinism}},
url = {http://www.journals.uchicago.edu/doi/10.1086/652433},
volume = {175},
year = {2010}
}
