isbn = {1461-0248 (Electronic)$\backslash$n1461-023X (Linking)},
issn = {1461023X},
journal = {Ecology Letters},
keywords = {Conservation biology,Evolutionary dead-end,Extinction and speciation,Generalist-to-specialist,Macroevolution,Niche breadth,Population decline,Resource-use hypothesis,Specialization-by-choice and specialization-by- co,Species lifetime,conservation biology,evolutionary dead-end,extinction and speciation,generalist-to-,macroevolution,niche breadth,population decline,resource-use hypothesis,specialist,specialization-by-choice and specialization-by-con,species lifetime},
number = {8},
pages = {849--863},
pmid = {19580588},
title = {{Are specialists at risk under environmental change? Neoecological, paleoecological and phylogenetic approaches}},
