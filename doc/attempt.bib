title = {{Modelling the ecological-functional diversification of marine Metazoa on geological time scales}},
url = {http://rsbl.royalsocietypublishing.org/cgi/doi/10.1098/rsbl.2011.0641},
volume = {8},
year = {2012}
}
@article{Rabosky2013,
