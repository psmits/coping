file = {:home/peter/Documents/Mendeley Desktop/Bush, Bambach, Daley/Paleobiology/Bush, Bambach, Daley - 2007 - Paleobiology.pdf:pdf;:home/peter/Documents/Mendeley Desktop/Bush, Bambach, Daley/Paleobiology/Bush, Bambach, Daley - 2007 - Paleobiology.pdf:pdf},
isbn = {0094-8373},
