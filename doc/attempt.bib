issn = {0027-8424},
journal = {Proceedings of the National Academy of Sciences},
number = {18},
pages = {7008--7012},
pmid = {22509033},
title = {{Tempo of trophic evolution and its impact on mammalian diversification}},
url = {http://www.pnas.org/cgi/doi/10.1073/pnas.1117133109},
volume = {109},
year = {2012}
}
@article{Ezard2016,
abstract = {Co-dependent geological and climatic changes obscure how species interact in deep time. The interplay between these environmental factors makes it hard to discern whether ecological compe-tition exerts an upper limit on species richness. Here, using the exceptional fossil record of Ceno-zoic Era macroperforate planktonic foraminifera, we assess the evidence for alternative modes of macroevolutionary competition. Our models support an environmentally dependent macroevolu-tionary form of contest competition that yields finite upper bounds on species richness. Models of biotic competition assuming unchanging environmental conditions were overwhelmingly rejected. In the best-supported model, temperature affects the per-lineage diversification rate, while both temperature and an environmental driver of sediment accumulation defines the upper limit. The support for contest competition implies that incumbency constrains species richness by restricting niche availability, and that the number of macroevolutionary niches varies as a function of envi-ronmental changes.},
author = {Ezard, Thomas H.G. and Purvis, Andy},
doi = {10.1111/ele.12626},
file = {:home/peter/Documents/Mendeley Desktop/Ezard, Purvis/Ecology Letters/Ezard, Purvis - 2016 - Ecology Letters.pdf:pdf;:home/peter/Documents/Mendeley Desktop/Ezard, Purvis/Ecology Letters/Ezard, Purvis - 2016 - Ecology Letters(2).pdf:pdf},
isbn = {1461-0248},
issn = {14610248},
journal = {Ecology Letters},
keywords = {Beverton-Holt,Contest competition,Diversification,Diversity-dependence,Ecological limits,Microfossil,Ricker,Scramble competition},
pages = {1--8},
pmid = {27278857},
title = {{Environmental changes define ecological limits to species richness and reveal the mode of macroevolutionary competition}},
year = {2016}
}
@article{Badgley2013,
abstract = {{\textless}p{\textgreater}In modern ecosystems, regions of topographic heterogeneity, when compared with nearby topographically homogeneous regions, support high species densities of mammals and other groups. This biogeographic pattern could be explained by either greater diversification rates or greater accommodation of species in topographically complex regions. In this context, we assess the hypothesis that changes in landscape history have stimulated diversification in mammals. Landscape history includes tectonic and climatic processes that influence topographic complexity at regional scales. We evaluated the influence of changes in topographic complexity and climate on origination and extinction rates of rodents, the most diverse clade of mammals.{\textless}/p{\textgreater}},
