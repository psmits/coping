isbn = {0022-2372},
issn = {00222372},
journal = {Journal of Mammalogy},
number = {3},
pages = {804},
title = {{New Species of Batodonoides (Lipotyphla, Geolabididae) from the Early Eocene of Wyoming: Smallest Known Mammal?}},
